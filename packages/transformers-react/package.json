{
  "name": "@huggingface/transformers-react",
  "version": "1.0.0",
  "description": "React hooks for Hugging Face Transformers.js",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./browser": {
      "types": "./dist/index.d.ts",
      "import": "./dist/browser.js"
    },
    "./browser.min": {
      "types": "./dist/index.d.ts",
      "import": "./dist/browser.min.js"
    }
  },
  "scripts": {
    "build": "node scripts/build.mjs && npm run typegen",
    "dev": "node scripts/dev.mjs",
    "typegen": "tsc --build",
    "clean": "rm -rf dist",
    "test": "jest"
  },
  "peerDependencies": {
    "@huggingface/transformers": "^4.0.0",
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@huggingface/transformers": "*",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.8.3"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "transformers",
    "transformers.js",
    "react",
    "hooks",
    "huggingface",
    "machine-learning",
    "ai"
  ],
  "author": "Hugging Face",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huggingface/transformers.js.git",
    "directory": "packages/transformers-react"
  },
  "publishConfig": {
    "access": "public"
  }
}
